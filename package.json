    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "fetch": "^1.1.0",
    "git-diff-parser": "^1.0.0",
    "glob-to-regexp": "^0.4.1",
    "js-yaml": "^3.14.0",
    "node-fetch": "^3.1.1",
    "parse-diff": "^0.7.0",
    "probot": "^9.5.3"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
